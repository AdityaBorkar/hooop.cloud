#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/aditya/projects/@sst-console/node_modules/.pnpm/sst@2.43.3_@aws-sdk+client-sso-oidc@3.609.0_@aws-sdk+client-sts@3.609.0_aws-crt@1.21.3__aws-c_ihwf444gnurjrgfzo467xxlhrq/node_modules/sst/cli/node_modules:/home/aditya/projects/@sst-console/node_modules/.pnpm/sst@2.43.3_@aws-sdk+client-sso-oidc@3.609.0_@aws-sdk+client-sts@3.609.0_aws-crt@1.21.3__aws-c_ihwf444gnurjrgfzo467xxlhrq/node_modules/sst/node_modules:/home/aditya/projects/@sst-console/node_modules/.pnpm/sst@2.43.3_@aws-sdk+client-sso-oidc@3.609.0_@aws-sdk+client-sts@3.609.0_aws-crt@1.21.3__aws-c_ihwf444gnurjrgfzo467xxlhrq/node_modules:/home/aditya/projects/@sst-console/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/aditya/projects/@sst-console/node_modules/.pnpm/sst@2.43.3_@aws-sdk+client-sso-oidc@3.609.0_@aws-sdk+client-sts@3.609.0_aws-crt@1.21.3__aws-c_ihwf444gnurjrgfzo467xxlhrq/node_modules/sst/cli/node_modules:/home/aditya/projects/@sst-console/node_modules/.pnpm/sst@2.43.3_@aws-sdk+client-sso-oidc@3.609.0_@aws-sdk+client-sts@3.609.0_aws-crt@1.21.3__aws-c_ihwf444gnurjrgfzo467xxlhrq/node_modules/sst/node_modules:/home/aditya/projects/@sst-console/node_modules/.pnpm/sst@2.43.3_@aws-sdk+client-sso-oidc@3.609.0_@aws-sdk+client-sts@3.609.0_aws-crt@1.21.3__aws-c_ihwf444gnurjrgfzo467xxlhrq/node_modules:/home/aditya/projects/@sst-console/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../sst/cli/sst.js" "$@"
else
  exec node  "$basedir/../sst/cli/sst.js" "$@"
fi
